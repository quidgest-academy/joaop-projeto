{
  "name": "clientapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "set NODE_OPTIONS=--max_old_space_size=10240 && vite build --out-dir dist --mode production",
    "build:dev": "set NODE_OPTIONS=--max_old_space_size=8192 && vite build --out-dir dist --mode development",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue src --fix",
    "vite:run": "set NODE_OPTIONS=--max_old_space_size=10240 && vite",
    "vite:serve": "set NODE_OPTIONS=--max_old_space_size=10240 && vite serve",
    "vite:preview": "set NODE_OPTIONS=--max_old_space_size=10240 && vite preview"
  },
  "dependencies": {
    "@quidgest/ui": "0.15.9",
    "@r2rka/vue3-treeselect": "0.2.4",
    "@vuepic/vue-datepicker": "3.0.0",
    "bootbox": "5.5.2",
    "bootstrap": "4.6.1",
    "d3": "7.0.0",
    "dagre-d3": "0.6.4",
    "jquery": "3.5.1",
    "jquery-highlight": "3.5.0",
    "jquery-ui": "1.12.1",
    "jquery.flot": "0.8.3",
    "lodash-es": "4.17.20",
    "mitt": "2.1.0",
    "moment": "2.24.0",
    "popper.js": "1.16.1",
    "vue": "3.4.20",
    "vue-i18n": "9.9.1",
    "vue-router": "4.2.2",
    "vuex": "4.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.19.0",
    "eslint-plugin-vue": "^9.33.0",
    "prettier": "^3.2.5",
    "sass": "^1.77.6",
    "svgstore": "3.0.1",
    "typescript": "^5.8.2",
    "vite": "^5.1.4"
  }
}
